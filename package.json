    "vue-gravatar": "^1.3.1",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.3",
    "vuex-persist": "^2.2.0",
    "web3": "^1.2.8"
  },
