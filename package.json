{
  "name": "tiny-boxes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "contract": "nodemon --exec 'npx oz compile' -e sol --verbose",
    "dev": "npm run lambda && npm run docs:build && npm run serve",
    "docs:build": "vuepress build ./src/docs",
    "docs:dev": "vuepress dev ./src/docs",
    "lambda": "netlify-lambda build ./src/lambda -c ./webpack.functions.js",
    "postinstall": "netlify-lambda install ./src/lambda",
    "production": "npm run build && npm run lambda"
  },
  "dependencies": {
